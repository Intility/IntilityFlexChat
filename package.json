{
    "name": "@intility/flex-chat",
    "description": "Wrapped component with basic setup and theming based on Twilios WebChat Component",
    "version": "1.1.10",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "author": {
        "name": "Christian Marker / Intility AS",
        "email": "christian.marker@intility.no"
    },
    "contributors": [
        {
            "name": "Eva Maria DahlÃ¸ / Intility AS",
            "email": "eva.maria.dahlo@intility.no"
        }
    ],
    "license": "MIT",
    "deprecated": false,
    "repository": {
        "url": "https://github.com/Intility/IntilityFlexChat"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "run-s lint clean:build build:compile build:types",
        "build:watch": "babel src --out-dir dist --extensions \".ts,.tsx\" --watch --copy-files",
        "build:compile": "babel src --out-dir dist --extensions \".ts,.tsx\" --copy-files",
        "build:types": "tsc --project tsconfig.types.json",
        "clean:build": "rimraf dist",
        "lint": "tsc && eslint src/**/*.ts[x]",
        "lint:fix": "eslint src/**/*.ts[x] --fix",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^5.13.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.28",
        "@fortawesome/react-fontawesome": "^0.1.9",
        "@twilio/flex-webchat-ui": "^2.5.0",
        "react-scripts": "3.4.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.12.37",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "husky": "^4.2.5",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "rimraf": "^3.0.2",
        "typescript": "^3.9.2"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/react",
            "@babel/preset-typescript"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "echo -e \"\\e[34mRunning Precommit hook \\e[39m \" && npm run lint:fix"
        }
    }
}
